        //=================================================================================
        //Load Mission Applications
        //=================================================================================
        var searchText = "";
        var pageSize=5;
        var pageNumber=1;
        function loadStories() {
        $.ajax({
            type: "POST",
            url: "/Admin/GetStories",
            dataType: "html",
            cache: false,
            data: {searchText:searchText,pageNumber:pageNumber,pageSize:pageSize},
            success: function (data) {
                $("#admin-story-table").html("");
